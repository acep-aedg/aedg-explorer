- tab = 'demographics'

= turbo_frame_tag "community_tab_content" do
  - if @community.show_population?
    #population.mb-5
      = render 'shared/section_header',
        icon: 'people-fill',
        title: 'Population',
        description: 'Demographic breakdown including age, gender, and employment status',
        anchor: '#population'
      
      = render "communities/#{tab}/population"
  
