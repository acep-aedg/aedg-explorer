%div{ data: { controller: 'datatables', datatables: { order_value: [0, 'desc']} } }
  %table.table.table-striped.border
    %thead
      %tr
        %th Start Year
        %th End Year
        %th E Households Total
        %th E Household Median Income
        %th E Household Income Under $10,000
        %th E Household Income $10,000 - $14,999
        %th E Household Income $15,000 - $24,999
        %th E Household Income $25,000 - $34,999
        %th E Household Income $35,000 - $49,999
        %th E Household Income $50,000 - $74,999
        %th E Household Income $75,000 - $99,999
        %th E Household Income $100,000 - $149,999
        %th E Household Income $150,000 - $199,999
        %th E Household Income $200,000+
    %tbody
      - @community.household_incomes.each do |income|
        %tr
          %td= income.start_year
          %td= income.end_year
          %td= income.e_households_total
          %td= income.e_household_median_income
          %td= income.e_household_inc_under_10000
          %td= income.e_household_inc_10000_14999
          %td= income.e_household_inc_15000_24999
          %td= income.e_household_inc_25000_34999
          %td= income.e_household_inc_35000_49999
          %td= income.e_household_inc_50000_74999
          %td= income.e_household_inc_75000_99999
          %td= income.e_household_inc_100000_149999
          %td= income.e_household_inc_150000_199999
          %td= income.e_household_inc_200000_plus