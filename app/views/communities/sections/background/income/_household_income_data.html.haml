%div{ data: { controller: 'datatables' }}
  %table.table.table-striped.border
    %thead
      %tr
        %th start_year
        %th end_year
        %th e_households_total
        %th e_household_median_income
        %th e_household_inc_under_10000
        %th e_household_inc_10000_14999
